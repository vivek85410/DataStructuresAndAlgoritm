#include<bits/stdc++.h>

int removeDuplicates(vector<int> &arr, int n) {
	set<int>ans;
	int count=0;
	for(int i=0;i<n;i++){
		ans.insert(arr[i]);		
	}
	for(int i=0;i<ans.size();i++){
		count++;
	}
	return count;
}
