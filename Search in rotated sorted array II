code link - https://leetcode.com/problems/search-in-rotated-sorted-array-ii/?envType=problem-list-v2&envId=binary-search


class Solution {
public:
    bool search(vector<int>& nums, int target) {
        sort(nums.begin(),nums.end());
        
        int start = 0;
        int end = nums.size()-1;
        int mid = start + (end-start) /2;
        
        while(start<=end){
            if(nums[mid]==target){
                return true;
            }else if(target<nums[mid]){
                end = mid-1;
            }else if(target>nums[mid]){
                start = mid+1;
            }
            mid = start + (end-start)/2;
        }
        return false;
    }
};
