code link - https://www.geeksforgeeks.org/problems/remove-all-characters-other-than-alphabets4923/1



class Solution{
public:
    string removeSpecialCharacter(string s) {
        // code here
        string res = "";
        for(int i=0;i<s.length();i++){
            if(s[i]>=65 && s[i]<= 90 || s[i]>=97 && s[i]<=122){
                res.push_back(s[i]);
            }
        }
        if(res == ""){
            return "-1";
        }else{
         return res;   
        }
        
    }
};
