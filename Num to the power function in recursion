#include<iostream>
using namespace std;

int power(int a, int b){
    // base case
    if(b==0){
        return 1;
    }

    if(b==1){
        return a;
    }

    //recursive call
    int ans = power(a,b/2);

    //if a is even
    if(b%2 == 0){
        return ans*ans;
    }
    else{
        //if b is odd
        return a*ans*ans;
    }
}

int main(){
    int a,b;
    cin>>a>>b;
    int result = power(a,b);
    cout<<result<<endl;
    return 0;
}
