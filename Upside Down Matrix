code link - https://www.codechef.com/practice/course/matrices/MATRICES/problems/MATTREVERSE


#include <bits/stdc++.h>
using namespace std;

int main() {
	int n,m;
	cin>>n>>m;
	int arr[n][m];
	for(int i=0;i<n;i++){
	    for(int j=0;j<m;j++){
	        cin>>arr[i][j];
	    }
	}
	int startingRow = 0;
	int endingRow = n-1;
	int startingCol = 0;
	int endingCol = m-1;
	
	for(int i=endingRow;i>=startingRow;i--){
	    for(int j=startingCol;j<=endingCol;j++){
	        cout<<arr[i][j]<<" ";
	    }
	    cout<<endl;
	}
	
return 0;
}
