code link - https://www.geeksforgeeks.org/problems/remove-spaces0128/1


class Solution {
  public:
    string modify(string& s) {
        // code here.
        string result = "";
        for(int i=0;i<s.length();i++){
            if(s[i]==' '){
                continue;
            }
            else{
                result.push_back(s[i]);
            }
        }
        return result;
    }
};
